{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": ["index.js", "./classes/"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "plugins": [],
  "opts": {
      "template": "./node_modules/docdash",
      "encoding": "utf8",
      "destination": "docs/",
      "readme": "./README.md",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "scripts": []
  }
}